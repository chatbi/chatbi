{
  "name": "chatbi",
  "version": "0.1.0",
  "description": "ChatBI is a BI system interacted by chat.",
  "main": "index.js",
  "scripts": {
    "dev:client": "pnpm run -C packages/client dev",
    "gen-api": "pnpm run -C packages/client api:init",
    "format": "prettier --write \"**/src/**/*.ts\" \"packages/server/test/**/*.ts\"",
    "lint": "eslint \"**/{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chatbi/chatbi.git"
  },
  "keywords": [
    "chatbi",
    "chatgpt",
    "llm",
    "data-visualization",
    "bi"
  ],
  "author": "yugasun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chatbi/chatbi/issues"
  },
  "homepage": "https://github.com/chatbi/chatbi#readme",
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "debug": "^4.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.11.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "engine": {
    "node": ">=18.12.0"
  }
